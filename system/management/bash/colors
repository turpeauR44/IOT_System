#!/bin/bash
noir='\e[0;30m'
gris='\e[1;30m'
rougefonce='\e[0;31m'
	stop=$rougefonce
rose='\e[1;31m'
vertfonce='\e[0;32m'
	processing=$vertfonce
vertclair='\e[1;32m'
orange='\e[0;33m'
	question=$orange
jaune='\e[1;33m'
	step=$jaune
bleufonce='\e[0;34m'
bleuclair='\e[1;34m'
violetfonce='\e[0;35m'
violetclair='\e[1;35m'
	value=$violetclair
cyanfonce='\e[0;36m'
cyanclair='\e[1;36m'
grisclair='\e[0;37m'
blanc='\e[1;37m'
	result=$blanc
neutre='\e[0;m'
